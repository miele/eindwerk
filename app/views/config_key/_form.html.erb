<fieldset>
						<legend>Social Media Keys</legend>
						<hr/>
						<h2>Twitter</h2>
						<hr/>
						<p>
						<label>Twitter Consumer Key:</label>
						<%= f.text_field(:twitter_consumer_key,{:class => "lf"})%>
						</p>
						<p>
						<label>Twitter Consumer Secret:</label>
						<%= f.text_field(:twitter_consumer_secret,{:class => "lf"})%>
						</p>
						<p>
						<label>Twitter Oauth Token:</label>
						<%= f.text_field(:twitter_oauth_token,{:class => "lf"})%>
						</p>
						<p>
						<label>Twitter Oauth Secret:</label>
						<%= f.text_field(:twitter_oauth_secret,{:class => "lf"})%>
						</p>
						<hr/>
						<h2>Facebook</h2>
						<hr/>
						<p>
						<label>Facebook fan page:</label>
						<%= f.text_field(:facebook_fan_page,{:class => "lf"})%>
						</p>
						<p>
						<label>Facebook Access Token:</label>
						<%= f.text_field(:facebook_access_token,{:class => "lf"})%>
						</p>
						<hr/>
						<h2>Google Agenda</h2>
						<hr/>
						<p>
						<label>Google Email:</label>
						<%= f.text_field(:google_email,{:class => "lf"})%>
						</p>
						<p>
						<label>Google Email Password:</label>
						<%= f.password_field(:google_email_password,{:class => "lf"})%>
						</p>				
						
						