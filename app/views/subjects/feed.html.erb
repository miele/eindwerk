<% @events.sort{|a,b| b.start_time <=> a.start_time}.each do |event| %>
	<article>
	<a class="boxgrid box alpha tl tooltip" href="#" title="<%= event.title %>">
			<h4><%= event.start_time.strftime("%d") %></h4>
			<h4><%= event.start_time.strftime("%m") %></h4>
			<p><%= event.title %></p>
			<p><%= event.start_time.strftime("%I:%M %p") %> - <%= event.end_time.strftime("%I:%M %p") %></p>
			<p><%= event.where %></p>
			</a>
	</article>
<% end %>
