<div data-role="header" data-position="inline" data-backbtn="false">
	<h1>Music</h1>
	<a href="/convert" data-icon="gear" class="ui-btn-right" data-transition="flip">Pagina's</a>
</div>

<div data-role="content">
<% @tracks.each do |track|%>
<div data-role="collapsible" data-state="collapsed">
		<h3><%= track.name %></h3>
	 <p><audio src="<%= track.song.url %>" controls="controls"></audio></p>
</div>
  <% end %>
</div>